json{
    "extends": [
        "config:base"
    ],
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "enabledManagers": [
        "npm",
        "gomod"
    ],
    "baseBranches": [
        "master"
    ],
    "labels": [
        "renovate"
    ],
    "ignorePaths": [
        "infrastructure/**",
        "search/services/internallinkboxes/**"
    ],
    "packageRules": [],  
    "npm": {
        "extends": [
            "helpers:disableTypesNodeMajor"
        ],
        "postUpdateOptions": [
            "npmDedupe"
        ]
    },
    "prHourlyLimit": 0,
    "prConcurrentLimit": 0
}