{
  "name": "raw-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@azure/msal-browser": "2.28.3",
    "@azure/msal-react": "1.4.7",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@improbable-eng/grpc-web": "0.15.0",
    "@material-ui/core": "5.0.0-alpha.34",
    "@material-ui/icons": "5.0.0-alpha.34",
    "@peculiar/webcrypto": "1.4.0",
    "@types/react-beforeunload": "2.1.1",
    "classnames": "2.3.2",
    "material-components-web": "14.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.4.0",
    "react-scripts": "5.0.1",
    "react-spinners": "0.13.8",
    "react-tooltip": "5.18.1",
    "react-use-sub": "3.0.0",
    "rxjs": "7.8.1"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test --color",
    "start": "react-scripts start",
    "eslint": "eslint --cache --cache-location 'misc/.eslintcache' --ext .ts,.tsx src/",
    "eslint-fix": "eslint --fix --cache --cache-location 'misc/.eslintcache' --ext .ts,.tsx src/",
    "circular-check": "madge -c --extensions ts,tsx --ts-config tsconfig.json --no-spinner src/",
    "lint-scss": "prettier -c --parser scss 'src/**/*.scss'",
    "format-scss": "prettier --write --parser scss 'src/**/*.scss'"
  },
  "devDependencies": {
    "@babel/core": "7.19.1",
    "@babel/plugin-syntax-flow": "7.18.6",
    "@babel/plugin-transform-react-jsx": "7.19.0",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "20.8.9",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/parser": "6.0.0",
    "browser-headers": "0.4.1",
    "eslint": "8.38.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-no-type-assertion": "1.3.0",
    "eslint-plugin-no-only-tests": "3.1.0",
    "eslint-plugin-prettier": "5.0.0",
    "google-protobuf": "3.21.0",
    "long": "5.2.0",
    "madge": "6.0.0",
    "prettier": "3.0.3",
    "protobufjs": "7.2.3",
    "react-refresh": "0.14.0",
    "sass": "^1.32.5",
    "spy4js": "3.4.1",
    "ts-proto": "1.125.0",
    "typescript": "5.3.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "eslint"
      ],
      "allowedVersions": {
        "react": "18",
        "react-dom": "18",
        "@types/react": "18"
      }
    },
    "overrides": {
      "react-error-overlay": "6.0.9"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://kuberpult-frontend-service:8081"
}
